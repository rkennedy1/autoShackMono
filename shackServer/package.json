{
  "name": "shack-server",
  "version": "1.0.0",
  "main": "build/index.js",
  "license": "MIT",
  "scripts": {
    "dev": "ts-node-dev --respawn --pretty --transpile-only index.ts",
    "start": "NODE_PATH=./build node build/index.js",
    "build": "tsc -p .",
    "build:clean": "rm -rf build && yarn build",
    "swagger": "node ./swagger.ts",
    "type-check": "tsc --noEmit",
    "health": "curl -f http://localhost:1783/health || exit 1"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.20.0",
    "express-rate-limit": "^7.2.0",
    "mysql2": "^3.15.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/parser": "^6.18.0",
    "swagger-autogen": "^2.23.7",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
